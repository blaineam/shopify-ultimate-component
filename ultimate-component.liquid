<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
<div class="ultimate-component-container {{ section.settings.class_name | defaut: ""}}" style="position:relative;">
  <div class="ultimate-component-block"> 
    {% if section.blocks.size > 0 %}
    <div class="ultimate-compnent-item-{{ section.id }} ultimate-component-wrapper"> 
      {% for block in section.blocks %}
      <div class="ultimate-component-block" {{ block.shopify_attributes }}>
        {% if block.settings.top_edge_image != blank %}
        <div style="top:0;z-index:5;height: {{ block.settings.top_edge_height | default: '10vh' }} !important;" class="ultimate-component-edges-container">
          <img class="ultimate-component-cover lazyload" {% if section.settings.lazyload %}data-{% endif %}src=" {{ block.settings.top_edge_image | img_url: 'master' }}" alt="Ultimate Component Top Edge" />
        </div> 
        {% endif %}
        <a href="{{ block.settings.link }}" class="ultimate-component-hyperlink same-height">
          <div class="ultimate-component-link"> 
            {% if block.settings.video_url != blank %}
            {% if block.settings.video_fallback_image != blank %} 
            <img style="z-index:0;" class="ultimate-component-cover lazyload"
            {% if section.settings.lazyload %}data-{% endif %}src=" {{ block.settings.video_fallback_image | img_url: 'master' }}" 
            alt="Ultimate Component Fallback Image" /> 
            {% endif %} 
            <video 
            autoplay 
            {% if block.settings.video_loop %}loop{% endif %} 
            {% if block.settings.video_fallback_image != blank %}poster="{{ block.settings.video_fallback_image | img_url: 'master' }}"{% endif %} 
            muted 
            playsinline 
            width="100%" 
            style="z-index:0;"
            class="ultimate-component-cover lazyload">
              <source {% if section.settings.lazyload %}data-{% endif %}src="{{ block.settings.video_url }}" type="video/mp4" />
            </video> 
            {% endif %}
            {% if block.settings.image != blank %} 
            <img style="z-index:1;" class="ultimate-component-cover ultimate-component-background-image {% if block.settings.mobile_image == blank %}  ultimate-component-background-image-mobile {% endif %}   lazyload"
            data-src=" {{ block.settings.image | img_url: 'master' }}" 
            alt="Ultimate Component Background Image" /> 
            {% endif %} 
            {% if block.settings.mobile_image != blank %} 
            <img style="z-index:1;" class="ultimate-component-cover ultimate-component-background-image-mobile  lazyload"
            {% if section.settings.lazyload %}data-{% endif %}src="{{ block.settings.mobile_image | img_url: 'master'}}" 
            alt="Ultimate Component Background Mobile Image" /> 
            {% endif %} 
            {% if block.settings.background_overlay != blank %}
            <div style="z-index:2;width:100%;height:100%;top:0;left:0;position:absolute;background:{{ block.settings.background_overlay }};"></div> {% endif %}
            {% if block.settings.image_overlay != blank %} 
            <img style="z-index:3;" class="ultimate-component-cover ultimate-component-image-overlay
              {% if block.settings.image_overlay_use_animation and block.settings.image_overlay_animation != blank  %}
                animate__animated
                animate__{{ block.settings.image_overlay_animation }}
                {% if block.settings.image_overlay_animation_delay != blank %}
                  animate__delay-{{block.settings.image_overlay_animation_delay}}
                {% endif %}
                {% if block.settings.image_overlay_animation_speed != blank %}
                  animate__{{block.settings.image_overlay_animation_speed}}
                {% endif %}
                {% if block.settings.image_overlay_animation_repeat != blank %}
                  animate__{{block.settings.image_overlay_animation_repeat}}
                {% endif %}
              {% endif %}

              {% if block.settings.paralax_driver != blank and block.settings.image_overlay_paralax_preset != blank %}
                lax  
                ultimate-component-animate-{{block.settings.paralax_driver}}
                lax_preset_{{block.settings.image_overlay_paralax_preset}}{% if block.settings.image_overlay_paralax_distance != blank and block.settings.image_overlay_paralax_strength  %}:{{block.settings.image_overlay_paralax_distance}}:{{block.settings.image_overlay_paralax_strength}}{% endif %}
              {% endif %}
               lazyload" {% if section.settings.lazyload %}data-{% endif %}src=" {{ block.settings.image_overlay | img_url: 'master' }}" alt="Ultimate Component Image Overlay" /> 
            {% endif %}
            {% if block.settings.subtitle != blank 
            or block.settings.subtitle_html != blank 
            or block.settings.maintitle != blank 
            or block.settings.maintitle_html != blank 
            or block.settings.desc != blank 
            or block.settings.desc_html != blank 
            or block.settings.button_label != blank %}
            <div class="ultimate-component-page-width" style="
                z-index: 4;
                padding-top: {{ block.settings.padding_top | default: '0px' }};
                padding-left: {{ block.settings.padding_left | default: '0px' }};
                padding-bottom: {{ block.settings.padding_bottom | default: '0px' }};
                padding-right: {{ block.settings.padding_right | default: '0px' }};
              ">
              <div class="ultimate-component-content ultimate-component-content-{{block.settings.text_position}} ultimate-component-mobile-content-{{block.settings.mobile_text_position}} ultimate-component-content-{{block.settings.layout}} ultimate-component-text-alignment-{{ block.settings.textalignment }}  ultimate-component-mobile-text-alignment-{{ block.settings.mobile_textalignment }}"
                style="{% if block.settings.max_width != empty %}
                  width: {{ block.settings.max_width }};
                  {% endif %}
                "> 
                {% if block.settings.subtitle != blank %}
                <div class="
                  ultimate-container-sub-title
                  
                  {% if block.settings.subtitle_css_classes != blank %}
                    {{ block.settings.subtitle_css_classes }}
                  {% endif %}
                  {% if block.settings.subtitle_use_animation and block.settings.subtitle_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.subtitle_animation }}
                    {% if block.settings.subtitle_animation_delay != blank %}
                      animate__delay-{{block.settings.subtitle_animation_delay}}
                    {% endif %}
                    {% if block.settings.subtitle_animation_speed != blank %}
                      animate__{{block.settings.subtitle_animation_speed}}
                    {% endif %}
                    {% if block.settings.subtitle_animation_repeat != blank %}
                      animate__{{block.settings.subtitle_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.subtitle_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.subtitle_paralax_preset}}{% if block.settings.subtitle_paralax_distance != blank and block.settings.subtitle_paralax_strength  %}:{{block.settings.subtitle_paralax_distance}}:{{block.settings.subtitle_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.subtitlecolor }}"> {{ block.settings.subtitle }}</div> 
                {% else %}
                <div class="
                  ultimate-container-sub-title
                  
                  {% if block.settings.subtitle_css_classes != blank %}
                    {{ block.settings.subtitle_css_classes }}
                  {% endif %}
                  {% if block.settings.subtitle_use_animation and block.settings.subtitle_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.subtitle_animation }}
                    {% if block.settings.subtitle_animation_delay != blank %}
                      animate__delay-{{block.settings.subtitle_animation_delay}}
                    {% endif %}
                    {% if block.settings.subtitle_animation_speed != blank %}
                      animate__{{block.settings.subtitle_animation_speed}}
                    {% endif %}
                    {% if block.settings.subtitle_animation_repeat != blank %}
                      animate__{{block.settings.subtitle_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.subtitle_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.subtitle_paralax_preset}}{% if block.settings.subtitle_paralax_distance != blank and block.settings.subtitle_paralax_strength  %}:{{block.settings.subtitle_paralax_distance}}:{{block.settings.subtitle_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.subtitlecolor }}"> {{ block.settings.subtitle_html }}</div>
                {% endif %}
                {% if block.settings.maintitle != blank %}
                <div class="
                  ultimate-component-main-title
                  
                  {% if block.settings.maintitle_css_classes != blank %}
                    {{ block.settings.maintitle_css_classes }}
                  {% endif %}
                  {% if block.settings.heading_use_animation and block.settings.heading_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.heading_animation }}
                    {% if block.settings.heading_animation_delay != blank %}
                      animate__delay-{{block.settings.heading_animation_delay}}
                    {% endif %}
                    {% if block.settings.heading_animation_speed != blank %}
                      animate__{{block.settings.heading_animation_speed}}
                    {% endif %}
                    {% if block.settings.heading_animation_repeat != blank %}
                      animate__{{block.settings.heading_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.heading_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.heading_paralax_preset}}{% if block.settings.heading_paralax_distance != blank and block.settings.heading_paralax_strength  %}:{{block.settings.heading_paralax_distance}}:{{block.settings.heading_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.maintitlecolor }}">{{ block.settings.maintitle }}</div>
                {% else %}
                <div class="
                  ultimate-component-main-title
                  
                  {% if block.settings.maintitle_css_classes != blank %}
                    {{ block.settings.maintitle_css_classes }}
                  {% endif %}
                  {% if block.settings.heading_use_animation and block.settings.heading_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.heading_animation }}
                    {% if block.settings.heading_animation_delay != blank %}
                      animate__delay-{{block.settings.heading_animation_delay}}
                    {% endif %}
                    {% if block.settings.heading_animation_speed != blank %}
                      animate__{{block.settings.heading_animation_speed}}
                    {% endif %}
                    {% if block.settings.heading_animation_repeat != blank %}
                      animate__{{block.settings.heading_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.heading_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.heading_paralax_preset}}{% if block.settings.heading_paralax_distance != blank and block.settings.heading_paralax_strength  %}:{{block.settings.heading_paralax_distance}}:{{block.settings.heading_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.maintitlecolor }}">{{ block.settings.maintitle_html }}</div>
                {% endif %}
                {% if block.settings.desc != blank %}
                <div class="
                  ultimate-component-desc
                
                  {% if block.settings.desc_css_classes != blank %}
                    {{ block.settings.desc_css_classes }}
                  {% endif %}
                  {% if block.settings.description_use_animation and block.settings.description_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.description_animation }}
                    {% if block.settings.description_animation_delay != blank %}
                      animate__delay-{{block.settings.description_animation_delay}}
                    {% endif %}
                    {% if block.settings.description_animation_speed != blank %}
                      animate__{{block.settings.description_animation_speed}}
                    {% endif %}
                    {% if block.settings.description_animation_repeat != blank %}
                      animate__{{block.settings.description_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.description_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.description_paralax_preset}}{% if block.settings.description_paralax_distance != blank and block.settings.description_paralax_strength  %}:{{block.settings.description_paralax_distance}}:{{block.settings.description_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.desccolor }}">{{ block.settings.desc }}</div>
                {% else %}
                <div class="
                  ultimate-component-desc
                
                  {% if block.settings.desc_css_classes != blank %}
                    {{ block.settings.desc_css_classes }}
                  {% endif %}
                  {% if block.settings.description_use_animation and block.settings.description_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.description_animation }}
                    {% if block.settings.description_animation_delay != blank %}
                      animate__delay-{{block.settings.description_animation_delay}}
                    {% endif %}
                    {% if block.settings.description_animation_speed != blank %}
                      animate__{{block.settings.description_animation_speed}}
                    {% endif %}
                    {% if block.settings.description_animation_repeat != blank %}
                      animate__{{block.settings.description_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.description_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.description_paralax_preset}}{% if block.settings.description_paralax_distance != blank and block.settings.description_paralax_strength  %}:{{block.settings.description_paralax_distance}}:{{block.settings.description_paralax_strength}}{% endif %}
                  {% endif %}
                " style="color:{{ block.settings.desccolor }}">{{ block.settings.desc_html }}</div>
                {% endif %}
                {% if block.settings.button_label != blank %}
                <div class="
                  slider-btn
                  btn
                    
                  {% if block.settings.button_use_animation and block.settings.button_animation != blank  %}
                    animate__animated
                    animate__{{ block.settings.button_animation }}
                    {% if block.settings.button_animation_delay != blank %}
                      animate__delay-{{block.settings.button_animation_delay}}
                    {% endif %}
                    {% if block.settings.button_animation_speed != blank %}
                      animate__{{block.settings.button_animation_speed}}
                    {% endif %}
                    {% if block.settings.button_animation_repeat != blank %}
                      animate__{{block.settings.button_animation_repeat}}
                    {% endif %}
                  {% endif %}

                  {% if block.settings.paralax_driver != blank and block.settings.button_paralax_preset != blank %}
                    lax  
                    ultimate-component-animate-{{block.settings.paralax_driver}}
                    lax_preset_{{block.settings.button_paralax_preset}}{% if block.settings.button_paralax_distance != blank and block.settings.button_paralax_strength  %}:{{block.settings.button_paralax_distance}}:{{block.settings.button_paralax_strength}}{% endif %}
                  {% endif %}
                ">{{ block.settings.button_label | escape }}</div>
                {% endif %}
              </div>
            </div> 
            {% endif %}
          </div>
        </a> 
        {% if block.settings.bottom_edge_image != blank %}
        <div style="bottom:0;z-index:5;height: {{ block.settings.bottom_edge_height | default: '10vh' }} !important;" class="ultimate-component-edges-container">
          <img class="ultimate-component-cover lazyload" {% if section.settings.lazyload %}data-{% endif %}src=" {{ block.settings.bottom_edge_image | img_url: 'master' }}" alt="Ultimate Component Bottom Edge" />
        </div> 
        {%endif %} 
      </div> 
      {% endfor %} 
    </div> 
    {% endif %} 
  </div>
</div>
